            <td>@Html.TextBoxFor(o => o.City)</td>
        </tr>
        <tr>
            <td>Region</td>
            <td>@Html.TextBoxFor(o => o.Region)</td>
        </tr>
        <tr>
            <td>PostalCode</td>
            <td>@Html.TextBoxFor(o => o.PostalCode)</td>
        </tr>
        <tr>
            <td>Country</td>
            <td>@Html.TextBoxFor(o => o.Country)</td>
        </tr>
        <tr>
            <td>Phone</td>
            <td>@Html.TextBoxFor(o => o.Phone)</td>
        </tr>
        <tr>

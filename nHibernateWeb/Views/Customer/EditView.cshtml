@using nHibernateWeb.Models
@model CustomerModel
@{
    ViewBag.Title = "EditView";
    //CustomerModel cusItem = (CustomerModel)ViewData["cusItem"];
}
@Styles.Render("~/Content/Site.css")
<h2>EditView</h2>

<div>
    <br />
    <table style="border:1px">
        <tr>
            <td>CustomerID</td>
            <td>@Html.TextBoxFor(o => o.CustomerID)</td>
        </tr>
        <tr>
            <td>CompanyName</td>
            <td>@Html.TextBoxFor(o => o.CompanyName)</td>
        </tr>
        <tr>
            <td>ContactName</td>
            <td>@Html.TextBoxFor(o => o.ContactName)</td>
        </tr>
        <tr>
            <td>ContactTitle</td>
            <td>@Html.TextBoxFor(o => o.ContactTitle)</td>
        </tr>
        <tr>
            <td>Address</td>
